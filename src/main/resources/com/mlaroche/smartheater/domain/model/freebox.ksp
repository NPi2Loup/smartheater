    field bytesDown    { domain: DoBytes,     label: "Total déscendant" }
    field bytesUp      { domain: DoBytes,     label: "Total montant" }
    field bandwidthDown{ domain: DoBytes,     label: "Max descendant (Kb/s)" }
    field bandwidthUp  { domain: DoBytes,     label: "Max montant (Kb/s)" }
