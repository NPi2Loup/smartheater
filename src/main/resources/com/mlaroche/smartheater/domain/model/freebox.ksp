    field tempT2      { domain: DoNumber,     label: "Température 2" }
    field tempCpuB      { domain: DoNumber,     label: "Température CPU B" }
    field fan0Speed      { domain: DoNumber,     label: "Ventilateur 1" }
    field rateDown     { domain: DoNumber,     label: "Débit déscendant" }
